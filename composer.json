{
    "name": "inextensodigital/security-bundle",
    "description": "Signed requests check",
    "keywords": ["security", "symfony", "bundle"],
    "type": "symfony-bundle",
    "homepage": "https://github.com/inextensodigital/SecurityBundle",
    "license": "MIT",
    "authors": [{
        "name": "Community contributors",
        "homepage": "https://github.com/inextensodigital/SecurityBundle/contributors"
    }],
    "require": {
        "php": ">7.1.3",
        "symfony/framework-bundle": "~2.6|~3.0|~4.0|~5.0",
        "symfony/security-bundle" : "~2.6|~3.0|~4.0|~5.0",
        "doctrine/common": "~2.2|~3.0"
    },
    "require-dev": {
        "atoum/atoum": "~3.0",
        "behat/behat": "~3.0",
        "behat/symfony2-extension": "~2.0",
        "behat/mink-extension": "~2.0",
        "behat/mink-goutte-driver": "~1.1"
    },
    "suggest": {
        "psr/http-message": "Required by \\Rezzza\\SecurityBundle\\Request\\Psr7RequestSigner"
    },
    "autoload":     {
        "psr-0": { "Rezzza\\SecurityBundle": "" }
    },
    "target-dir": "Rezzza/SecurityBundle",
    "extra": {
        "branch-alias": {
            "dev-master": "2.0.x-dev"
        }
    },
    "config": {
        "bin-dir": "bin"
    }
}
